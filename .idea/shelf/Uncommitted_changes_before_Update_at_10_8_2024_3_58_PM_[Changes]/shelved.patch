-+    private Text regexAccepted; // Text for accepted regular expression
-+    @FXML
-+    private Text languageAccepted; // Text for accepted language
-+
-+    // Transition table: Map of current state and input to next state and tape action
-+    private Map<String, String[]> transitionTable;
-+
-+    public TMController() {
-+        // Initialize the transition table
-+        transitionTable = new HashMap<>();
-+        // Define transitions according to the regular expression 0+ c 0+
-+        transitionTable.put("0,0", new String[]{"0", "0", "R"}); // q0 on 0 -> q0, write 0, move right
-+        transitionTable.put("0,c", new String[]{"1", "c", "R"}); // q0 on 'c' -> q1, write 'c', move right
-+        transitionTable.put("1,0", new String[]{"1", "0", "R"}); // q1 on 0 -> q1, write 0, move right
-+        transitionTable.put("1,blank", new String[]{"2", "blank", "L"}); // q1 on blank (end of input) -> q2, move left
-+        transitionTable.put("2,0", new String[]{"2", "0", "L"}); // q2 on 0 -> q2, write 0, move left
-+        transitionTable.put("2,c", new String[]{"3", "c", "R"}); // q2 on 'c' -> q3, write 'c', move right
-+        transitionTable.put("3,0", new String[]{"3", "0", "R"}); // q3 on 0 -> q3, write 0, move right
