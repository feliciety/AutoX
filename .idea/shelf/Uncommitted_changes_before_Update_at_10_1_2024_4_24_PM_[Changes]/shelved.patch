+    // Helper method to initialize tape with a binary string
+    private void initializeTape(String binaryResult) {
+        tapeContainer.getChildren().clear(); // Clear previous tape content
+        for (char c : binaryResult.toCharArray()) {
+            TextField tapeCell = new TextField(String.valueOf(c));
+            tapeCell.setPrefWidth(40);  // Set fixed width for each cell
+            tapeCell.setEditable(false);  // Make the cells read-only
+            tapeContainer.getChildren().add(tapeCell);  // Add the cell to the tape
+        }
