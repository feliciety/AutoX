+    public static String addBinary(String a, String b) {
+        int num1 = Integer.parseInt(a, 2);
+        int num2 = Integer.parseInt(b, 2);
+        int sum = num1 + num2;
+        return Integer.toBinaryString(sum);
+    }
+
+    public static String subtractBinary(String a, String b) {
+        int num1 = Integer.parseInt(a, 2);
+        int num2 = Integer.parseInt(b, 2);
+
+        if (num1 < num2) {
+            return "Error: Result is negative. Subtraction of a smaller number from a larger one.";
+        }
+
+        int difference = num1 - num2;
+        return Integer.toBinaryString(difference);
+    }
+
+    public static String multiplyBinary(String a, String b) {
+        int num1 = Integer.parseInt(a, 2);
+        int num2 = Integer.parseInt(b, 2);
+        int product = num1 * num2;
+        return Integer.toBinaryString(product);
+    }
+
+    public static String[] divideBinary(String a, String b) {
+        int num1 = Integer.parseInt(a, 2);
+        int num2 = Integer.parseInt(b, 2);
+
+        if (num2 == 0) {
+            return new String[]{"Error: Division by zero."};
+        }
+
+        int quotient = num1 / num2;
+        int remainder = num1 % num2;
+
+        return new String[]{Integer.toBinaryString(quotient), Integer.toBinaryString(remainder)};
+    }
+
+    public static void showTransitions(String binary1, String binary2, String operation) {
+        System.out.println("-- Turing Machine Transitions --");
+        System.out.println("Transitioning through " + operation + " for inputs: " + binary1 + " and " + binary2);
+
+        for (int i = 0; i < Math.max(binary1.length(), binary2.length()); i++) {
+            String state = "q" + (i % 6); // Example state representation
+            String currentSymbol1 = i < binary1.length() ? String.valueOf(binary1.charAt(i)) : " ";
+            String currentSymbol2 = i < binary2.length() ? String.valueOf(binary2.charAt(i)) : " ";
+            System.out.println("State: " + state + ", Current Symbols: " + currentSymbol1 + " and " + currentSymbol2);
+        }
+    }
